{
  "name": "tachikomas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build-utils": "node --inspect=9999 --require ts-node/register scripts/build-learn/index.ts",
    "build-shared": "cd packages/shared && pnpm build",
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write --cache packages",
    "lint": "eslint packages --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --cache",
    "lint:fix": "pnpm run lint --fix",
    "changeset": "changeset",
    "release": "changeset publish",
    "prepare": "husky install",
    "postinstall": "npx husky install .husky",
    "publish": "changeset publish --registry=https://registry.npmjs.com/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx,md,vue}": "eslint --fix --cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/node": "^20.4.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "cz-git": "^1.3.11",
    "czg": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^15.1.0",
    "prettier": "3.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "vite": "^4.4.5"
  }
}
