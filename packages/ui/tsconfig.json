{
    "compilerOptions": {
      "baseUrl": ".",
      "jsx": "preserve",
      "strict": true,
      "target": "ES2015",
      "module": "ESNext",
      "skipLibCheck": true,
      "esModuleInterop": true,
      "moduleResolution": "Node",
      "lib": ["esnext", "dom"],
      "resolveJsonModule": true,
      "outDir": "dist",
      "types": ["unplugin-vue-define-options/macros-global"]
    },
    "include": ["src/**/*", "./shims-vue.d.ts"],
    "exclude": [
        "node_modules",
         "**/dist/**",
         "./unocss.config.ts"
    ],
    "allowSyntheticDefaultImports": "true"
}
